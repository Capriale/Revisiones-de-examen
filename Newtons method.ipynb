   "source": [
    "def newtons_method(num_iterations=1000):\n",
    "    x = 0\n",
    "    for i in range(num_iterations):\n",
    "        gradient = 6 * x + 2\n",
    "        hessian = 6 \n",
    "        x = x - gradient / hessian\n",
    "    return x"
   ]
  },
