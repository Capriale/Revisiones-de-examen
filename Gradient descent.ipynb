   "source": [
    "def gradient_descent(learning_rate=0.01, num_iterations=1000):\n",
    "    x = 0\n",
    "    for i in range(num_iterations):\n",
    "        gradient = 6 * x + 2 \n",
    "        x = x - learning_rate * gradient\n",
    "    return x\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
